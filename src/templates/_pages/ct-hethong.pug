							.form-group.col-12.col-md-6
								input(type="text" placeholder="Tiêu đề...")
							.form-group.col-12.col-md-6
								input(type="text" placeholder="Số điện thoại...")
							.form-group.col-12
								textarea(name="", cols="", rows="5" placeholder="Nội dung...")
